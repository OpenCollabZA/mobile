'use strict';
var LearningUnitsListCtrl = ($scope, $rootScope, $routeParams, LearningUnitsService, SynthErrorHandler) => {

    			$rootScope.activePage = 'learning_units';
    			$rootScope.breadcrumbs = [{'name' : 'Learning Units'}];
    			$scope.moduleId = $routeParams.moduleId;

    // Put all announcements on UI
    			LearningUnitsService
        .getUnits($routeParams.moduleId)
        .then(function(units) {
            			$scope.units = units;
        }, SynthErrorHandler);
};
LearningUnitsListCtrl.$inject = ['$scope', '$rootScope', '$routeParams', 'LearningUnitsService', 'SynthErrorHandler'];
export default LearningUnitsListCtrl;
