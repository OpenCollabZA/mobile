'use strict';

import FaqListCtrl from './controllers/FaqListCtrl';
import FaqDetailCtrl from './controllers/FaqDetailCtrl';

import FaqService from './services/FaqService';

import Routes from './routes';

var FaqTool = 'synthesis.tools.faq',
	controllers = 'synthesis.tools.faq.controllers',
	factories = 'synthesis.tools.faq.factories';


angular.module(controllers, [])
	.controller('FaqListCtrl', FaqListCtrl)
	.controller('FaqDetailCtrl', FaqDetailCtrl);

angular.module(factories, [])
	.factory('FaqService', FaqService);

angular.module(FaqTool, [
	controllers,
	factories
]).config(Routes);

export default FaqTool;
