'use strict';
/*
 * tools/faq/routes.js
 */
var Config = ($routeProvider/*, SynthEmbeddedImageHandlerProvider, SynthLinkHandlerProvider*/) => {

	//SynthEmbeddedImageHandlerProvider.addHandler('learning_units', 'LearningUnitsImageHandler');
	//SynthLinkHandlerProvider.addHandler('learning_units', 'LearningUnitsLinkHandler');

	$routeProvider
		.when('/tool/learning_units/:moduleId', 					{templateUrl : 'tools/learning_units/partials/unit-list.html'})
		.when('/tool/learning_units/:moduleId/:unitId',				{templateUrl : 'tools/learning_units/partials/viewUnit.html'})
		.when('/tool/learning_units/:moduleId/:unitId/:sectionId',	{templateUrl : 'tools/learning_units/partials/viewSection.html'});
};
Config.$inject = ['$routeProvider'/*, 'SynthEmbeddedImageHandlerProvider', 'SynthLinkHandlerProvider'*/];
export default Config;
